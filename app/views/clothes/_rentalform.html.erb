<%= simple_form_for [@clothe, @rental], data: { controller: 'flatpickr', action: "submit->form#submit"} do |f| %>
  <div class="row">
  <div class="col-6"><%= f.input :start_date, label: 'Date de début', as: :string, error:"Ce champ doit être rempli", input_html: { data: { flatpickr_target: "startTime" } }  %></div>
  <div class="col-6"><%= f.input :end_date, label: 'Date de fin', as: :string, error:"Ce champ doit être rempli", input_html: { data: { flatpickr_target: "endTime" } }  %></div>
  </div>
  <%= f.submit 'Louer', class: "btn btn-default btn-primary btn-lg" %>
  <% end %>
